<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.apache.iotdb.admin.mapper.DemoUserMapper">

  <select id="selectUserInfo" resultType="org.apache.iotdb.admin.model.entity.DemoUser" parameterType="java.lang.Integer">
        select  a.*
        from demo_user as a  join user_info as b
        on a.id=b.user_id
    </select>
</mapper>
